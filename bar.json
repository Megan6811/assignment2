{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "background": "black",
    "config": {
      "title": {
        "color": "whitesmoke",
        "fontSize": 50
      },
      "axis": {
        "labelColor": "whitesmoke",
        "titleColor": "whitesmoke"
      }
    },
    "data": {
      "url": "https://raw.githubusercontent.com/Megan6811/assignment2/main/humans.csv"
    },
    "hconcat": [
      {
        "width": 300,
        "height": 400,
        "mark": "bar",
        "encoding": {
          "x": {"field": "State", "title": null},
          "y": {
            "field": "Homes Lost",
            "type": "quantitative",
            "title": "Homes Lost"
          },
          "color": {"value": "chocolate"}
        }
      },
      {
        "width": 300,
        "height": 400,
        "mark": "bar",
        "encoding": {
          "x": {"field": "State", "title": null},
          "y": {
            "field": "Fatalities",
            "type": "quantitative",
            "title": "Fatalities"
          },
          "color": {"value": "chocolate"}
        }
      }
    ],
    "title": {
      "text": "Bushfire Effects on Humans:",
      "color": "whitesmoke"
    }
  }  