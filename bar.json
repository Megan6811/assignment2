{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/Megan6811/assignment2/main/humans.csv"
    },
    "layer": [
      {
        "mark": "bar",
        "encoding": {
          "x": {"field": "State"},
          "y": {"field": "Value", "type": "quantitative", "scale": {"domain": [0, 2500]}, "title": "Homes Lost"},
          "color": {"value": "green"}
        }
      },
      {
        "mark": "bar",
        "encoding": {
          "x": {"field": "State"},
          "y": {"field": "Value", "type": "quantitative", "scale": {"domain": [0, 500]}, "title": "Fatalities"},
          "color": {"value": "red"}
        }
      }
    ],
    "title": "Grouped Bar Chart for Fatalities and Homes Lost by State"
  }  