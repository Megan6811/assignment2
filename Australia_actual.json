{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 450,
    "title": "Australian Bushfire Satellite Data",
    "projection": {"type": "equalEarth"},
    "layer": [
    {
    "data": {
    "url":"https://raw.githubusercontent.com/alwaysblazing/Australia-State-TopoJson-MapChart/master/au-states-topo.json",
    "format": {"type": "topojson", "feature":
    "ne_110m_admin_0_countries"}
    },
    "mark": {"type": "geoshape", "fill": "lightgray", "stroke":
    "lightgray"}
    },
    {
        "data": {
        "url":
        "https://raw.githubusercontent.com/Megan6811/assignment2/447d9c24b6df98bdea3436f59b9001cf02fc51ea/fire_nrt_M6_101673.csv"
        },
        "mark": {"type": "circle"},
        "encoding": {
        "longitude": {"field": "longitude", "type":
        "quantitative"},
        "latitude": {"field": "latitude", "type":
        "quantitative"},
        "color": {
        "field": "frp",
        "type": "quantitative",
        "title": "frp",
        "scale": {"scheme": "reds"}
        }
        }
        }
        ]
        }
        